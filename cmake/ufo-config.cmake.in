@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/ufo-targets.cmake")

foreach(_comp ${UFO_FIND_COMPONENTS})
  if(TARGET UFO::${_comp})
    set(UFO_${_comp}_FOUND TRUE)
  else()
    set(UFO_${_comp}_FOUND FALSE)
  endif()
endforeach()

include(CMakeFindDependencyMacro)
find_dependency(Threads) 
find_dependency(TBB) 
find_dependency(OpenMP)

include("${CMAKE_CURRENT_LIST_DIR}/Compute-deps.cmake")
# Force it to recognize this specific wgpu_native target
# This ensures that even if it was built with an absolute path originally, 
# it now points to our relocatable 'wgpu_native' target made in Compute-deps.
# set_property(TARGET UFO::Compute APPEND PROPERTY INTERFACE_LINK_LIBRARIES wgpu_native)

#checks if any REQUIRED components were marked FOUND=FALSE and sets UFO_FOUND
check_required_components(UFO)